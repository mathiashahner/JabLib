#include <modInclude.h>

char * CelciusToKelvin (float temperature)
{
float result;

   result = temperature + 273.15;

   return ( ConvertToString( result ));
}

char * ConvertToString( float valor )
{
char * result;
char buffer[20];

   result = sprintf( buffer, "%.2f", valor );

   return result;
}
